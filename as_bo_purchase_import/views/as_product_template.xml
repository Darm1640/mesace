<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="as_product_template_import" model="ir.ui.view">
            <field name="name">as.product.template.form</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_only_form_view" />
            <field name="type">form</field>
            <field name="priority" eval="40" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='property_account_expense_id']" position="after">
                    <field name="property_account_import_id" />
                </xpath> 
            </field>
        </record>
    </data>
</odoo>