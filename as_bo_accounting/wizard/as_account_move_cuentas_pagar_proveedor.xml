<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <data>
            <record id="view_stock_quantity_history_pagos" model="ir.ui.view">
            <field name="name">Reporte de Inventario</field>
            <field name="model">as.stock.quantity.history</field>
            <field name="arch" type="xml">
               <form string="elige tu fecha">
                <group>
                    <group>
                        <field name="date" />
                    </group>
                </group>
                <footer>
                    <button name="open_table" string="Recuperar facturas a pagar" type="object" class="btn-primary"/>
                    <button string="Cancelar" class="btn-secondary" special="cancel" />
                </footer>
                </form>
            </field>
        </record>
    <record id="action_stock_quantity_history" model="ir.actions.act_window">
        <field name="name">CUENTA POR PAGAR A PROVEEDOR</field>
        <field name="res_model">as.stock.quantity.history</field>
        <field name="view_mode">tree,form</field>
        <field name="view_id" ref="view_stock_quantity_history_pagos"/>
        <field name="target">new</field>
    </record>
    <menuitem id="report_menu_valuation" name="Cuenta por pagar a proveedor" parent="as_assets_report_cuentas_por_pagar_main" sequence="100" action="action_stock_quantity_history"/>
    </data>
</odoo>