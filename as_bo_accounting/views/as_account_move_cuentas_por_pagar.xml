<?xml version="1.0" encoding="utf-8"?>
<odoo>
       <record id="as_account_view_account_view_cuentas_por_pagar" model="ir.ui.view">
            <field name="name">as.account.view.account.view.cuentas.por.pagar</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_in_invoice_tree" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='ref']" position="after">
                    <field name="as_proveedor_factura" />
                    <field name="as_fecha_vencimiento" />
                    <field name="as_fecha_factura" />
                    <field name="as_origin_invoice" />
                </xpath>
                
            </field>
        </record>
    <record id="as_view_picking_form" model="ir.ui.view">
        <field name="model">account.move</field>
        <field name="name">as.account.move.form</field>
        <field name="inherit_id" ref="account.view_invoice_tree" />
        <field name="arch" type="xml">
            <xpath expr="//tree/field[@name='invoice_date_due']" position="replace">
                <field name="invoice_date_due" string="Dias de vencimiento" invisible="True"/>
            </xpath>
        </field>
    </record>


</odoo>