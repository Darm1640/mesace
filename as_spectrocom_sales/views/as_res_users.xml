<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="as_view_res_users_form" model="ir.ui.view">
            <field name="name">res.users.pricelist</field>
            <field name="model">res.users</field>
            <field name="inherit_id" ref="base.view_users_form"/>
            <field name="arch" type="xml">
                <xpath expr="//group[@name='preferences']" position="after">
                     <group name="ventas" string="Ventas" >
                              <field name="as_sequence_id"/>
                        </group>
                </xpath>
            </field>
        </record>
    </data>
</openerp>