<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="as_view_picking_form2" model="ir.ui.view">
        <field name="model">stock.picking</field>
        <field name="name">as.stock.picking.form2</field>
        <field name="inherit_id" ref="stock.view_picking_form" />
        <field name="arch" type="xml">
            <xpath expr="//button[@name='button_validate']" position="after">
                    <button name="action_create_assets_2" type="object" string="Generar Activos 100"
                        class="oe_highlight" id="bid_confirm" attrs="{'invisible': [('as_activo_100_pick', '=', False)]}"/>
            </xpath>
            <xpath expr="//field[@name='origin']" position="after">
                      <field name="as_activo_100_pick" invisible="True"/>
            </xpath>
        </field>
    </record>
