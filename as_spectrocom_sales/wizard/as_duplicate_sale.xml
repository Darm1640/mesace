<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
    <record id="as_duplicar_ventas_asd_view" model="ir.ui.view">
        <field name="name">Duplicar ventas</field>
        <field name="model">as.duplicate.sale</field>
        <field name="arch" type="xml">
            <form string="Duplicar ventas">
                <group col="4">
                    
                    <field name="as_alias_lugar" />
                    <field name="as_partner_id" />
                    <!-- <button name="get_sale_process" string="Procesar" type="object" class="btn-primary" /> -->
                </group>
                <footer>
                    <button name="get_sale_process" type="object" default_focus="1" string="Duplicar"
                        context="{'xlsx_export':1}" icon="fas fa-folders" class="oe_highlight" />
                    or
                    <button string="Cancelar" class="oe_link" special="cancel" />
                </footer>
            </form>
        </field>
    </record>

    <record id="as_duplicar_ventas_asd_act" model="ir.actions.act_window">
        <field name="name">Duplicar ventas</field>
        <field name="res_model">as.duplicate.sale</field>
        <field name="view_mode">form</field>
        <field name="view_id" ref="as_duplicar_ventas_asd_view" />
        <field name="target">new</field>
    </record>
    
    </data>

</odoo>