<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
       
        <record id="as_product_name_tree" model="ir.ui.view">
            <field name="name">Producto Nombre</field>
            <field name="model">as.product.name</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name" />
                    <field name="as_taxes_id" widget="many2many_tags"/>
                    <field name="as_account_gasto_id" />
                   
                </tree>
            </field>
        </record>
        <record id="as_producto_name_form" model="ir.ui.view">
            <field name="name">Producto Nombre</field>
            <field name="model">as.product.name</field>
            <field name="arch" type="xml">
                <form>
                    <field name="name" />
                    <field name="as_taxes_id" widget="many2many_tags"/>
                    <field name="as_account_gasto_id" />
                   
                </form>
            </field>
        </record>
         <record id="as_producto_name_action_p" model="ir.actions.act_window">
            <field name="name">Nombre Servicios</field>
            <field name="res_model">as.product.name</field>
            <field name="view_mode">tree,form</field>
        </record>
        <menuitem id="as_producto_name_menu" parent="account.menu_finance_receivables" name="Nombre de Servicios" sequence="101"
            action="as_producto_name_action_p" />
        
    </data>
</odoo>