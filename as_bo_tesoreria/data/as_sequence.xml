<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="sequence_tesoereria" model="ir.sequence">
            <field name="name">Cajas de Tesoreria</field>
            <field name="code">account.tesoereria</field>
            <field name="prefix">CAJA/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>
         <record id="sequence_operacion" model="ir.sequence">
            <field name="name">Cajas de Tesoreria</field>
            <field name="code">account.operacion</field>
            <field name="prefix">OPERACION/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>
         <record id="sequence_caja_chica" model="ir.sequence">
            <field name="name">Cajas Chica</field>
            <field name="code">account.chica</field>
            <field name="prefix">Caja_Chica/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>
        <record id="sequence_check_day" model="ir.sequence">
            <field name="name">Cheques al Dia</field>
            <field name="code">account.checks.day</field>
            <field name="prefix">D-%(range_year)s-</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>
         <record id="sequence_check_date" model="ir.sequence">
            <field name="name">Cheques a la Fecha</field>
            <field name="code">account.checks.date</field>
            <field name="prefix">C-%(range_year)s-</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>
         <record id="sequence_cdeposit" model="ir.sequence">
            <field name="name">Cheques depositos</field>
            <field name="code">account.deposit</field>
            <field name="prefix">DE/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">5</field>
        </record>
    </data>
</odoo>
