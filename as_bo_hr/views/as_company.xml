<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_company_form_inherit_banner" model="ir.ui.view">
        <field name="name">res.company.form.inherit.banner</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='website']" position="after">
                <field name="as_numero_patronal" />
                <field name="as_patronal_municipal" />
            </xpath>
        </field>
    </record>

</odoo>
