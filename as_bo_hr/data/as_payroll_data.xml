        <record id="hr_rule_4" model="hr.salary.rule">
            <field name="struct_id" ref="as_bo_hr.structure_type_employee_employee" />
            <field name="name">Trabajo extraordinario y nocturno</field>
            <field name="sequence" eval="4" />
            <field name="code">HOURS100</field>
            <field name="category_id" ref="hr_payroll.BASIC" />
            <field name="condition_select">python</field>
            <field name="condition_python">result = inputs.HOURS100</field>
           <field name="amount_select">code</field>
            <field name="amount_python_compute">valor_hombre = 0.0
valor_mujer=0.0
valor=0.0
if employee.gender == 'male':
    valor = payslip.as_indicadores_id.as_horas_hombre*(payslip.as_indicadores_id.as_semanas_meses or 1)
else:
    valor = payslip.as_indicadores_id.as_horas_mujer*(payslip.as_indicadores_id.as_semanas_meses or 1)
factor = BASIC/(valor or 1)
reposicion = float(inputs.HOURS100.input_type_id.as_recargo)
sutotal = factor * float(inputs.HOURS100.amount)
result =  sutotal*reposicion</field>
