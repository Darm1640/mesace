            <field name="condition_python">result = payslip.as_antiguedad_payslib >= 2</field>
            <field name="amount_select">code</field>
            <field name="amount_python_compute">valor = 0.0
for item in payslip.as_indicadores_id.as_antiguedad_ids:
    if payslip.as_antiguedad_payslib >= item.as_desde and payslip.as_antiguedad_payslib &lt;= item.as_hasta:
        valor = item.as_valor
result = (float(payslip.as_smn_id.amount)*float(payslip.as_indicadores_id.as_factor_3smn))*(float(valor)/100)</field>
        </record>

