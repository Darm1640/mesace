<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_form_custodies_name" model="ir.ui.view">
            <field name="name">as.as.assets.custodies</field>
            <field name="model">as.assets.custodies</field>
            <field name="arch" type="xml">
                <form string="Custodios de Activos">
                    <sheet>
                        <group>
                            <field name="as_date_start" />
                            <field name="as_date_end" />
                            <field name="as_employee_id" />
                            <field name="as_job_id" />
                            <field name="as_department_id" />
                            <field name="as_location_id" />
                            <field name="as_product_activo_id" invisible="True" />
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="view_tree_custodies_name" model="ir.ui.view">
            <field name="name">as.as.assets.custodies</field>
            <field name="model">as.assets.custodies</field>
            <field name="arch" type="xml">
                <tree string="Custodios de Activos">
                    <field name="as_date_start" />
                    <field name="as_date_end" />
                    <field name="as_employee_id" />
                    <field name="as_job_id" />
                    <field name="as_department_id" />
                    <field name="as_location_id" />
                    <field name="as_product_activo_id" invisible="True"/>
                </tree>
            </field>
        </record>

        <record model="ir.actions.act_window" id="action_proposal_custodies_type">
            <field name="name">Custodios de Activos</field>
            <field name="res_model">as.account.sale</field>
            <field name="view_mode">tree,form</field>
        </record>

        <record id="view_form_account_name" model="ir.ui.view">
            <field name="name">as.account.sale</field>
            <field name="model">as.account.sale</field>
            <field name="arch" type="xml">
                <form string="Cuentas Contables">
                    <sheet>
                    <field name="name" />
                        <group string="Asiento 1">
                            <field name="as_account_1_debit_id" />
                            <field name="as_account_1_credit_id" />
                           
                        </group>
                        <group string="Asiento 2">
                            <field name="as_account_2_debit_id" />
                            <field name="as_account_2_credit_id" />
                           
                        </group>
                        <group string="Asiento Caso 3 y 4">
                            <field name="as_account_3_caj_id" />
                            <field name="as_account_3_itc_id" />
                            <field name="as_account_3_itp_id" />
                            <field name="as_account_3_dep_id" />
                            <field name="as_account_3_her_id" />
                            <field name="as_account_3_iva_id" />
                            <field name="as_account_3_oin_id" />
                           
                        </group>
                          <group string="Asiento Dar de Baja">
                            <field name="as_discharged_debit_id" />
                            <field name="as_discharged_credit_id" />
                           
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="view_tree_account_name" model="ir.ui.view">
            <field name="name">as.account.sale</field>
            <field name="model">as.account.sale</field>
            <field name="arch" type="xml">
                <tree string="Cuentas Contables">
                    <field name="name" />
                </tree>
            </field>
        </record>

        <record model="ir.actions.act_window" id="action_proposal_account_type">
            <field name="name">Cuentas Contables</field>
            <field name="res_model">as.account.sale</field>
            <field name="view_mode">tree,form</field>
        </record>




    </data>
</odoo>