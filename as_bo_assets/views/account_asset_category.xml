<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="as_product_template_account_form_view">
            <field name="name">account.asset.category.view.costs</field>
            <field name="model">account.asset.category</field>
            <field name="inherit_id" ref="om_account_asset.view_account_asset_category_form" />
             <field name="priority" eval="20" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='method_time']" position="before">
                    <field name="as_useful_life" />
                    <field name="as_coeficiente" />
                    <field name="as_dias_util" />
                    <field name="as_modality" />
                    <field name="as_assets_type" />
                </xpath>
                 <xpath expr="//field[@name='account_asset_id']" position="after">
                    <field name="account_asset_generic_id" />
                    <field name="as_date_inicio" />
                </xpath>
                 <xpath expr="//sheet" position="inside">
                 <group >
                 
                    <group string="Cuentas Contables Asientos de AF">
                    <field name="as_estructura_assets_id" />
                  </group>
                 </group>
                </xpath>
            </field>
        </record>

    </data>
</odoo>