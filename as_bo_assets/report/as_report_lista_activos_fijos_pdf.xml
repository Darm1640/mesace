<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="as_report_list_activos_fijos_pdf">
            <t t-call="web.html_container">
                <div class="header">
                    <style type="text/css">
                        .header {
                            margin: 0px;
                            padding: 0px;
                        }
                    </style>
                </div>
            </t>
            <t t-call="web.external_layout">
                <div class="page">
                    <style type="text/css">
                        .page {
                            margin: -75px 0px 8px 0px;
                            padding: 0px;
                        }
                    </style>
                    <div class="row mt0 mb0" style=" margin-left: 18px; margin-right: 18px;">
                        <div style="text-align:left;font-size:10px; width:50%;margin-top:12px;">

                            <h4 style="text-align: left; font-size:10px; line-height: 2px;">
                                <div>
                                <img t-att-src="image_data_uri(logo)"
                                        style="height: 90px; width: 250px; text-align: left; " />
                                    <br />
                                </div>
                            </h4>
                        </div>
                        <div class="col-2" style="text-align:left; width: 30%;margin-left: 18px; margin-right: 18px;"
                            name='as_titulo'>
                        </div>
                        <div style="text-align:left; width: 30%; margin-top:30px;margin-left: 18px; margin-right: 18px;"
                            name='as_titulo'>
                            <table width="100%" style="border:#fffff 1px solid;">
                                <tr>
                                    <td style="text-align: left; font-size:11px; width:100%; color:black; ">
                                        <b>NIT: </b>
                                    </td>
                                </tr>
                                <tr>
                                    <td style="text-align: left; font-size:11px; width:100%; color:black;">
                                        <b>DIRECCION: </b>
                                    </td>
                                </tr>
                                <tr>
                                    <td style="text-align: left; font-size:11px; width:100%; color:black;">
                                        <b>TELEFONO, CELULAR: </b>
                                    </td>
                                </tr>
                            </table>

                        </div>
                    </div>
                    <div style="margin-left: 18px; margin-right: 18px;margin-top:3px;">
                        <table width="100%">
                            <tr>
                                <td
                                    style="width:100%; text-align:center;font-size:15px;color:#4682B4;border:0.5pt solid black;vertical-align: middle;">
                                    <b>LISTA ACTIVOS FIJOS</b></td>
                            </tr>
                            
                        </table>
                    </div>
                    <div style="margin-left: 18px; margin-right: 18px;">
                        <table width="100%">
                            <tr style="width:100%;">
                                <td width="50%">
                                    <b style="text-align:left; font-size: 12px; color:#4682B4;">Usuario: </b>
                                    <b style="text-align:left; font-size: 12px; color:#black;"> <span
                                            t-esc="usuario" /></b>
                                </td>
                                
                                <td width="50%">
                                    <b style="text-align:left; font-size: 12px; color:#4682B4;">Fecha de impresion:
                                    </b><b style="text-align:left; font-size: 12px;"><span t-esc="fecha_actual" /></b>
                                </td>
                            </tr>
                            <tr style="width:100%;">

                                <td  width="50%">
                                    <b style="text-align:left; font-size: 12px; color:#4682B4;">Nº Serie: </b><b
                                        style="text-align:left; font-size: 12px;"> <span t-esc="series" /></b>
                                </td>
                                <td width="50%">
                                    <b style="text-align:left; font-size: 12px; color:#4682B4;">Productos: </b>
                                    <b style="text-align:left; font-size: 12px; color:#black;"><span
                                            t-esc="producto" /></b>
                                </td>

                            </tr>
                        </table>
                    </div>
                    <div style="margin-left: 18px; margin-right: 18px;">
                        <table width="100%">
                            <tr style="text-align:center;font-size:12px; color:#ffffff ;background-color:#4682B4;border:0.5pt solid black;vertical-align: middle;">
                                <td style="width:5%; border:0.5pt solid black; vertical-align: middle;"><b>Nª DE ITEMS</b></td>
                                <td style="width:8%; border:0.5pt solid black; vertical-align: middle;"><b>CODIGO</b></td>
                                <td style="width:10%; border:0.5pt solid black; vertical-align: middle;"><b>CODIGO ACTIVO FIJO</b></td>
                                <td style="width:25%; border:0.5pt solid black; vertical-align: middle;"><b>PRODUCTO</b></td>
                                <td style="width:10%; border:0.5pt solid black; vertical-align: middle;"><b>NRO DE SERIE</b></td>
                                <td style="width:8%; border:0.5pt solid black; vertical-align: middle;"><b>MARCA</b></td>
                                <td style="width:10%; border:0.5pt solid black; vertical-align: middle;"><b>MODELO</b></td>
                                <td style="width:10%; border:0.5pt solid black; vertical-align: middle;"><b>NRO DE PARTE</b></td>
                                <td style="width:14%; border:0.5pt solid black; vertical-align: middle;"><b>CATEGORIA</b></td>
                            </tr>
                        </table>
                    </div>
                    <div style="margin-left: 18px; margin-right: 18px;margin-bottom:18px;">
                        <table t-foreach="lista_salidas_inventarios" t-as="movimientos" width="100%">

                            <tr style="font-size:9px; color:black;border:0.1pt solid silver;">
                                <td style="width:5%;border:0.1pt solid silver;vertical-align: middle;">
                                    <p style="text-align:center">
                                        <t t-esc="movimientos['contador']" />
                                    </p>
                                </td>
                                <td style="width:8%;border:0.1pt solid silver;vertical-align: middle;">
                                    <p style="text-align:center;">
                                        <t t-esc="movimientos['val0']" />
                                    </p>
                                </td>
                                <td style="width:10%;border:0.1pt solid silver;vertical-align: middle;">
                                    <p style="text-align:center;">
                                        <t t-esc="movimientos['val1']" />
                                    </p>
                                </td>

                                <td style="width:25%;border:0.1pt solid silver;vertical-align: middle;">
                                    <p style="text-align:left;margin-left:2px;margin-right:2px;">
                                        <t t-esc="movimientos['val2']" />
                                    </p>
                                </td>

                                <td style="width:10%;border:0.1pt solid silver;vertical-align: middle;">
                                    <p style="text-align:center;">
                                        <t t-esc="movimientos['val3']" />
                                    </p>
                                </td>

                                <td style="width:8%;border:0.1pt solid silver;vertical-align: middle;">
                                    <p style="text-align:center;">
                                        <t t-esc="movimientos['val4']" />
                                    </p>
                                </td>
                                <td style="width:10%;border:0.1pt solid silver;vertical-align: middle;">
                                    <p style="text-align:left;margin-left:2px;margin-right:2px;font-size:9px;">
                                        <t t-esc="movimientos['val5']" />
                                    </p>
                                </td>
                                <td style="width:10%;border:0.1pt solid silver;vertical-align: middle;">
                                    <p style="text-align:left;margin-left:2px;margin-right:2px;font-size:10px;">
                                        <t t-esc="movimientos['val6']" />
                                    </p>
                                </td>
                                <td style="width:14%;border:0.1pt solid silver;vertical-align: middle;">
                                    <p style="text-align:left;margin-left:3px;font-size:10px;">
                                        <t t-esc="movimientos['val7']" />
                                    </p>
                                </td>
                                
                            </tr>
                        </table>
                    </div>
                </div>
            </t>
            <t t-foreach="docs" t-as="o">
                <div class="footer" style="font-family: 'Arial' !important;">
                    <div class="oe_structure" />
                    <div style="text-align:center;">
                    </div>
                </div>
            </t>
        </template>
    </data>
</odoo>