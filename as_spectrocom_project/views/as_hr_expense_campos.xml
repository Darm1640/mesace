<?xml version="1.0" encoding="utf-8"?>
<odoo>  
    <record id="as_gastos_agregar_campo_proyect" model="ir.ui.view">
        <field name="model">hr.expense.sheet</field>
        <field name="name">as.gastos.agregar.campo.proyect</field>
        <field name="inherit_id" ref="hr_expense.view_hr_expense_sheet_form" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='user_id']" position="after">
                    <field name="as_proyecto_id"/>
                    <field name="as_date_row"/>
                    <field name="as_cuenta_analitica"/>
            </xpath>
        </field>
    </record>   
</odoo>