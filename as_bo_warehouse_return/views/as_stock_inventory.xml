<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
         <record model="ir.ui.view" id="as_stock1_picking_form_view">
            <field name="name">as.stock.picking.form</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='move_ids_without_package']/tree/field[@name='quantity_done']"
                    position="after">
                    <field name="as_subtotal" sum="Total" invisible="True"/>
                  
                </xpath>  
               
            </field>
        </record>
         <record model="ir.ui.view" id="as_view_picking_sildec_form">
            <field name="name">as.stock.picking.form</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form" />
            <field eval="12" name="priority" />
            <field name="arch" type="xml">
               
                <xpath expr="//field[@name='origin']" position="after">
                      <field name="as_type_action" />
                </xpath> 
                <xpath expr="//field[@name='group_id']" position="after">
                      <field name="as_move_acion" />
                </xpath>
               
            </field>
        </record>
    </data>
</odoo>