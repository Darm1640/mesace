<?xml version="1.0" encoding="utf-8"?>
<odoo>
    
        
      <report
        id="as_reporte_existencias_pdf"
        string="Historial de Reparaciones (PDF)"
        model="as.historial.reparaciones"
        report_type="qweb-pdf"
        name="as_spectrocom_repair.as_historial_reparaciones_pdf"
        file="as_spectrocom_repair.as_historial_reparaciones_pdf"
        menu="False"
      />
      <record id="as_inventarios_nucleo_salidas_inventario" model="report.paperformat">
        <field name="name">Historial de Reparaciones PDF</field>
        <field name="default" eval="True" />
        <field name="format">custom</field>
        <field name="page_height">278</field>
        <field name="page_width">214</field>
        <field name="margin_top">7</field>
        <field name="margin_bottom">0</field>
        <field name="margin_left">0</field>
        <field name="margin_right">0</field>
        <field name="orientation">Landscape</field>
        <field name="header_spacing">0</field>
        <field name="report_ids" eval="[(4, ref('as_reporte_existencias_pdf'))]"/>
      </record> 

      <record model="ir.ui.menu" id="repair.repair_menu_reporting">
        <field name="groups_id" eval="[(4, ref('stock.group_stock_user'))]"/>
      </record>
</odoo>
