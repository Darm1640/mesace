<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data>
  <record id="as_boton_confirmado" model="ir.ui.view">
            <field name="name">as.boton.confirmado</field>
            <field name="model">repair.order</field>
            <field name="inherit_id" ref="repair.view_repair_order_form" />
            <field name="arch" type="xml">
                <xpath expr="//form/header" position="inside">
                    <button name="action_confirmado" string="IMPRIMIR RECEPCION DE EQUIPOS" type="object" class="oe_highlight" states="confirmed"/>
                </xpath>
            </field>
        </record>
        <report
        id="as_reporte_recepcion_equipos_pdf"
        string="Reporte de Recepcion de Equipos (PDF)"
        model="repair.order"
        report_type="qweb-pdf"
        name="as_spectrocom_repair.as_recepcion_equipos_pdf"
        file="as_spectrocom_repair.as_recepcion_equipos_pdf"
        menu="False"
      />
      <record id="as_recepcion_equipos" model="report.paperformat">
        <field name="name">Reporte de recepcion de equipos PDF</field>
        <field name="default" eval="True" />
        <field name="format">custom</field>
        <field name="page_height">278</field>
        <field name="page_width">214</field>
        <field name="margin_top">0</field>
        <field name="margin_bottom">0</field>
        <field name="margin_left">0</field>
        <field name="margin_right">0</field>
        <field name="orientation">Portrait</field>
        <field name="header_spacing">0</field>
        <field name="report_ids" eval="[(4, ref('as_spectrocom_repair.as_reporte_recepcion_equipos_pdf'))]"/>
      </record> 
</data>
</odoo>