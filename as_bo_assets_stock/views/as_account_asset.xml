<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="as_view_account_asset_asset_form111" model="ir.ui.view">
        <field name="model">account.asset.asset</field>
        <field name="name">as.account.asset.asset.form</field>
        <field name="inherit_id" ref="om_account_asset.view_account_asset_asset_form" />
        <field name="arch" type="xml">
         
            <xpath expr="//field[@name='date_first_depreciation']" position="after">
                <field name="as_picking_id"/>
                <field name="as_move_id"/>
            </xpath>
              <xpath expr="//field[@name='category_id']" position="before">
                        <!-- <field name="product_id" /> -->
                        <field name="as_code_assets" />
                        <field name="as_lot_id" />
                </xpath>

        </field>
    </record>
 
</odoo>
