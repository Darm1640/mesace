<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="as_endpoint_siat_tree" model="ir.ui.view">
            <field name="name">LOG para SIAT</field>
            <field name="model">as.siat.log</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name" />
                    <field name="as_user_id" />
                    <field name="as_success" />
                    <field name="as_estado" />
                    <field name="as_info" />
                    <field name="as_json" />
                    <field name="as_xml" />
                    <field name="as_date" />
                </tree>
            </field>
        </record>

        <record id="as_endpoint_siat_form" model="ir.ui.view">
            <field name="name">LOG para SIAT</field>
            <field name="model">as.siat.log</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name" />
                        </group>
                        <group>
                            <field name="as_user_id" />
                            <field name="as_success" />
                            <field name="as_estado" />
                            <field name="as_info" />
                            <field name="as_json" />
                            <field name="as_xml" />
                            <field name="as_date" />
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="as_endpoint_siat_action" model="ir.actions.act_window">
            <field name="name">LOG para SIAT</field>
            <field name="res_model">as.siat.log</field>
            <field name="view_mode">tree,form</field>
            <field name="view_id" ref="as_endpoint_siat_tree" />
        </record>


        <menuitem id="as_endpoint_siat_menu" action="as_endpoint_siat_action"
            parent="as_bo_invoice_client.as_invoice_bolivia_menu" name="LOG para SIAT" sequence="7"
            groups="account.group_account_manager" />

    </data>
</odoo>