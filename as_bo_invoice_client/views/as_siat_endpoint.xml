<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="as_endpoint_siat_tree2" model="ir.ui.view">
            <field name="name">Endpoints SIAT</field>
            <field name="model">as.siat.endpoint</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name" />
                    <field name="as_type" />
                </tree>
            </field>
        </record>

        <record id="as_endpoint_siat_form2" model="ir.ui.view">
            <field name="name">Endpoints SIAT</field>
            <field name="model">as.siat.endpoint</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name" />
                        </group>
                        <group>
                            <field name="as_type" />
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="as_endpoint_siat_action2" model="ir.actions.act_window">
            <field name="name">Endpoints SIAT</field>
            <field name="res_model">as.siat.endpoint</field>
            <field name="view_mode">tree,form</field>
            <field name="view_id" ref="as_endpoint_siat_tree2" />
        </record>


        <menuitem id="as_endpoint_siat_menu2" action="as_endpoint_siat_action2"
            parent="as_bo_invoice_client.as_invoice_bolivia_menu" name="Endpoints SIAT" sequence="3"
            groups="account.group_account_manager" />

    </data>
</odoo>