<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="as_invoice_sync_cron" model="ir.cron">
            <field name="name">Facturas por contingencia en cola</field>
            <field name="model_id" ref="model_as_send_massive_invoice"/>
            <field name="state">code</field>
            <field name="code">model._sync_adyen_cron()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
        </record>
    </data>
    <data noupdate="1">
        <record id="as_invoice_sync_cron1" model="ir.cron">
            <field name="name">Colocar paquetes que esten en borrador en cola</field>
            <field name="model_id" ref="model_as_send_massive_invoice"/>
            <field name="state">code</field>
            <field name="code">model._sync_adyen_cron_in_cola()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
        </record>
    </data>
</odoo>
